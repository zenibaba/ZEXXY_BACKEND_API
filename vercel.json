{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "GITHUB_TOKEN": "@github_token",
    "GITHUB_REPO_OWNER": "zenibaba",
    "GITHUB_REPO_NAME": "ZEXXY_KEYAUTH"
  }
}
